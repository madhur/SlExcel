                            <TextBlock x:Name="requiredfields" FontFamily="Arial" FontSize="13" Margin="25,6,0,0" Grid.Row="1" HorizontalAlignment="Left" VerticalAlignment="Top">                        
								<Run Text="All fields marked with" Foreground="#FF002663"/>
								<Run Text=" * " Foreground="#FFF30606"/>
								<Run Text="are required fields." Foreground="#FF002663"/>
                            </TextBlock>
                            <TextBlock x:Name="ideanameTxt" HorizontalAlignment="Left" Margin="25,8,0,0" Grid.Row="2" VerticalAlignment="Center" FontFamily="Arial" FontSize="12">
