                                <RadioButton GroupName="LOB1" x:Name="gbt_radio" Content="GBT" Grid.Column="1" HorizontalAlignment="Left" Margin="0" Grid.Row="4" VerticalAlignment="Top" Height="17" Width="60" Checked="LOBRadioButtons_Checked" Unchecked="LOBRadioButtons_Unchecked" FontFamily="Arial" FontSize="12"/>
                                <RadioButton GroupName="LOB1" x:Name="gcp_radio" Content="GCP" Grid.Column="1" HorizontalAlignment="Left" Margin="6,0,0,0" Grid.Row="4" VerticalAlignment="Top" Height="17" Width="60" Checked="LOBRadioButtons_Checked" Unchecked="LOBRadioButtons_Unchecked" FontFamily="Arial" FontSize="12"/>

