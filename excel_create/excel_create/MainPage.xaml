								<Run Text="  Idea Name"/>
								<LineBreak/>
								<Run FontSize="9" Text="     (Character Limit 60)"/>

                            </TextBlock>
                            <TextBox x:Name="ideaname" HorizontalAlignment="Left" TextChanged="Ideanametextchanged" Height="23" Margin="155,11,0,0" Grid.Row="2" TextWrapping="Wrap" VerticalAlignment="Top" Width="250" FontFamily="Arial" FontSize="12" MaxLength="60"/>


                            <TextBlock x:Name="descriptionTxt" HorizontalAlignment="Left" Margin="24,0,0,0" Grid.Row="3" TextWrapping="Wrap" VerticalAlignment="Center" FontFamily="Arial" FontSize="12">
								<Run Text="*" FontSize="14" Foreground="#FFF30606"/>   
								<Run Text="  Idea Description"/>
                                                 
								<LineBreak/>
								<Run FontSize="9" Text="      (Character Limit 255)"/>
                            </TextBlock>
                            <TextBox x:Name="description" HorizontalAlignment="Left" TextChanged="Descriptiontextchanged" Height="150" Margin="155,19,0,0" Grid.Row="3" TextWrapping="Wrap" VerticalAlignment="Top" Width="250" FontFamily="Arial" FontSize="12" MaxLength="255" Grid.RowSpan="3" VerticalScrollBarVisibility="Auto"/>

                            <TextBlock HorizontalAlignment="Left" Margin="12,6,0,0" Grid.Row="1" TextWrapping="Wrap" Text="Created By" VerticalAlignment="Top" Grid.Column="1" Height="16" Width="81" FontFamily="Arial" FontSize="12"/>
