                            <Border CornerRadius="5" Background="#FFF5F6F4" BorderThickness="1,1,3,2" BorderBrush="#FF002663" Margin="140,51,263,12" Grid.Row="3" Height="125" Width="323">
                                <StackPanel Margin="5" Height="115" Width="300">
                                    <Button x:Name="PopupButton13" Content="X" Click="PopUpButton13_Click" Cursor="Hand" FontFamily="Arial" FontSize="8" Background="{x:Null}" BorderBrush="#FF002663" Foreground="#FF002663" VerticalAlignment="Top" HorizontalAlignment="Right" VerticalContentAlignment="Top" Padding="0,2,3,3" Width="13" Height="12" HorizontalContentAlignment="Right" Margin="14,0,5,0" />
                                    <TextBlock x:Name="PopUpText_future" TextWrapping="Wrap" VerticalAlignment="Bottom" HorizontalAlignment="Center" FontFamily="Arial" Padding="1,2" Margin="1,2,1,4" Foreground="#FF002663" Width="300" Height="115"/>

                                </StackPanel>
                            </Border>
                        </Popup>-->


