                            <TextBlock x:Name="executorTxt" HorizontalAlignment="Left" Margin="0,14,0,0" Grid.Row="2" TextWrapping="Wrap" VerticalAlignment="Top" FontFamily="Arial" FontSize="12" Grid.Column="1">
                            
								<Run Text="*" FontSize="14" Foreground="#FFF30606"/>
								<Run Text="  Idea Executor"/>
                            </TextBlock>
                            <Controls1:PeopleChooser x:Name="SinglePeopleChooser" AllowMultiple="False"  Grid.Column="1" Margin="133,11,44,0" Grid.Row="2" Grid.RowSpan="2" HorizontalAlignment="Right"/>
