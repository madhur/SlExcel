<controls:ChildWindow x:Class="manage.Controls.FTEMsgBox"
           xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" 
           xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" 
           xmlns:controls="clr-namespace:System.Windows.Controls;assembly=System.Windows.Controls"
           Width="300" Height="100" 
           Title="" HasCloseButton="False">
    <Grid x:Name="LayoutRoot" Margin="2" Background="White">
        <Grid.RowDefinitions>
            <RowDefinition />
            <RowDefinition Height="Auto" MinHeight="35" />
        </Grid.RowDefinitions>
        <TextBlock Text="Are all FTE Contributors accounted for?" Foreground="#FF002663" HorizontalAlignment="Center" VerticalAlignment="Center" FontFamily="Arial" FontSize="14" Margin="0,13,0,0"/>
        <TextBlock x:Name="Result" Visibility="Collapsed"/>
        <Button x:Name="CancelButton" Content="No" Click="CancelButton_Click" HorizontalAlignment="Center" Margin="132,8,86,7" Grid.Row="1" VerticalAlignment="Center" Height="20" Width="60" Padding="3" FontFamily="Arial" FontSize="12" />
        <Button x:Name="OKButton" Content="Yes" Click="OKButton_Click" HorizontalAlignment="Center" Margin="70,8,148,7" Grid.Row="1" VerticalAlignment="Center" Height="20" Width="60" Padding="3" FontFamily="Arial" FontSize="12" />
    </Grid>
</controls:ChildWindow>